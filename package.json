{
  "name": "flip-curried",
  "version": "1.0.0",
  "description": "Flip like in Haskell.",
  "keywords": [
    "flip",
    "curry",
    "curried",
    "TypeScript",
    "Haskell"
  ],
  "author": {
    "name": "Simon Alling",
    "email": "alling.simon@gmail.com",
    "url": "https://simonalling.se"
  },
  "license": "MIT",
  "homepage": "https://github.com/simonalling/flip-curried",
  "repository": {
    "type": "git",
    "url": "https://github.com/simonalling/flip-curried"
  },
  "bugs": {
    "url": "https://github.com/simonalling/flip-curried"
  },
  "main": "dist/index",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "npm run clean && tsc -d -p . && npm run rename && tsc --module CommonJS -p .",
    "clean": "rm -rf dist/*",
    "lint": "tslint --project .",
    "prepublishOnly": "npm run verify",
    "rename": "renamer --force --find \"/\\.js$/\" --replace \".mjs\" \"dist/**\"",
    "test": "jest",
    "verify": "npm run lint && npm run build && npm test"
  },
  "sideEffects": false,
  "jest": {
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "testRegex": ".+\\.test\\.ts$",
    "moduleFileExtensions": [
      "ts",
      "js",
      "json",
      "node"
    ]
  },
  "devDependencies": {
    "@types/jest": "^24.0.18",
    "jest": "^24.9.0",
    "prettier": "^1.18.2",
    "renamer": "^1.1.2",
    "ts-jest": "^24.0.2",
    "tslint": "^5.19.0",
    "typescript": "^3.5.3"
  }
}
